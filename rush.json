/**
 * This is the main configuration file for Rush.
 * For full documentation, please see https://rushjs.io
 */
{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.64.0",
  "npmVersion": "6.14.10",
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "gitPolicy": {
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/sunnysingh/rush-sandbox",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "telemetryEnabled": false,
  "projects": [
    {
      "packageName": "@sunnysingh/rush-sandbox-odin",
      "projectFolder": "packages/odin",
      "shouldPublish": true
    },
    {
      "packageName": "@sunnysingh/rush-sandbox-thor",
      "projectFolder": "packages/thor",
      "shouldPublish": true
    }
  ]
}
